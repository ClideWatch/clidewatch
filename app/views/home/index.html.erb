</table>

<% end %>


<table id="contentTable">
  <tr>
    <td id="navTd">
      <table id="navTable">
        <tr><td onClick="parent.contentFrame.location = 'clides'"   >Stats</td   ></tr>
        <tr><td onClick="parent.contentFrame.location = 'ratings'"  >Ratingss</td></tr>
        <tr><td onClick="parent.contentFrame.location = 'reviews'"  >Reviewss</td></tr>
        <tr><td onClick="parent.contentFrame.location = 'faqs.html'">FAQs</td    ></tr>
      </table>
    </td>
    <td id="contentTd"><iframe id="contentFrame" src="welcome.html"></iframe></td>
  </tr>
</table>
