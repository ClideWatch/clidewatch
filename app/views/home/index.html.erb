  <tr>
    <td id="bannerTd">
      <pre id="asciiBannerPre"><%= ASCII_ART_BANNERS[0] %></pre>
      <span id="siteDescSpan">
          <%=raw '<b           id="promptLoginB"    >clide@guide</b>'      +
                 '<span        id="promptPathSpan"  >:~/cloud/ide/</span>' +
                 '$ watch<span id="promptCursorSpan">_</span>'             %></span>
